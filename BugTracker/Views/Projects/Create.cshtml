@model BugTracker.Models.Projects
<div class="row">
    <div class="col-md-12">
        <div class="col-md-3"></div>
        <div class="well bs-component col-md-6">
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()
                <fieldset>
                    <legend>Create Project</legend>
                    @*@Html.ValidationSummary(true, "", new { @class = "text-danger" })*@
                    <div class="form-group">
                        <div class="col-md-12">
                            <p>
                                <input name="Name" type="text" class="form-control" placeholder="Project Name">
                                @Html.ValidationMessageFor(m => m.Name, "", new { @class = "text-danger" })
                            </p>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-12">
                            <p>
                                <input name="StartDate" type="datetime" class="form-control" placeholder="Start Date">
                                @Html.ValidationMessageFor(m => m.StartDate, "", new { @class = "text-danger" })
                            </p>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-12">
                            <p>
                                <input name="EndDate" type="datetime" class="form-control" placeholder="End Date">
                                @Html.ValidationMessageFor(m => m.EndDate, "", new { @class = "text-danger" })
                            </p>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-12">
                            <button type="submit" class="btn btn-primary">Create</button>
                        </div>
                    </div>
                </fieldset>
            }
        </div>
        <div class="col-md-3"></div>
    </div>
</div>
 <div class="row">
    <div class="col-md-12">
        <div class="col-md-3"></div>
        <div class="col-md-6">
            @Html.ActionLink("Back to List", "Index")
        </div>
        <div class="col-md-3"></div>
    </div>
</div>